  "version": "0.2.0",
  "configurations": [{
    "runtimeExecutable": "/usr/bin/brave",
    "name": "Launch Chrome with ng serve",
    "type": "chrome",
    "request": "launch",
    "url": "http://localhost:4200",
    "webRoot": "${workspaceFolder}",
    "sourceMapPathOverrides": {
      "webRoot": "${workspaceFolder}",
      "webpack:/*": "${workspaceFolder}/*"
    }
  }, ]
}
